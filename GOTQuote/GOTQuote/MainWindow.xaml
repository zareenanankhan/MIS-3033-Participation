<Window x:Class="GOTQuote.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GOTQuote"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="windowimage">
        <Grid.Background>
            <ImageBrush ImageSource="https://hips.hearstapps.com/hmg-prod.s3.amazonaws.com/images/game-of-thrones-illustration-sean-longmore-1598631176.jpeg?crop=0.669xw:1.00xh;0.0896xw,0&amp;resize=640:*"/>
        </Grid.Background>
        <Button x:Name="buttonget" Content="Get Quote" HorizontalAlignment="Left" Height="94" Margin="34,10,0,0" VerticalAlignment="Top" Width="171" IsDefault="True" Click="buttonget_Click"/>
        <TextBlock x:Name="textquote" HorizontalAlignment="Left" Height="101" Margin="34,305,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="702" FontSize="22" FontWeight="Bold"/>

    </Grid>
</Window>
